<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("head") %>
        <link rel="stylesheet" href="css/start.css">
        <title>Start page</title>
</head>

<body>
    <div class="scroll">
        <% if (locals.images.length !=0) {
            images.forEach(image=> { %>
            <div><img src="images/Slides/<%- image %>"></div>
            <% }) 
        } %>
    </div>
    <footer>
        <a href="/">
            <h1 class="click">Tap here to start!</h1>
        </a>
    </footer>
    <%- include("tail") %>
        <script src="js/start.js"></script>
</body>

</html>